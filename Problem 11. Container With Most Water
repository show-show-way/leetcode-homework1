class Solution {
public:
    int maxArea(vector<int>& height) {
        int left = 0;
        int right = height.size() - 1;
        int maxArea = 0;

        while (left != right) {
            int aheight = min(height[left], height[right]) ;
            int currentArea = aheight * (right - left);
            maxArea = max(maxArea, currentArea);

            if (height[left] < height[right]) {
                left++;
            } 
            else {
                right--;
            }
        }

        return maxArea;
    }
};
